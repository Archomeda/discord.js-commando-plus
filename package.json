{
  "name": "discord.js-commando-plus",
  "version": "11.2.0",
  "description": "An unofficial fork of discord.js-commando",
  "repository": "Archomeda/discord.js-commando-plus",
  "author": "Archomeda",
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "bot",
    "commands"
  ],
  "scripts": {
    "test": "\"$npm_execpath\" run lint && \"$npm_execpath\" run docs:test",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "docs": "docgen --source src --custom docs/index.yml --output docs/docs.json --jsdoc jsdoc.json",
    "docs:test": "docgen --source src --custom docs/index.yml --jsdoc jsdoc.json"
  },
  "main": "src/index",
  "types": "./typings/index.d.ts",
  "dependencies": {
    "common-tags": "^1.0.0",
    "deepmerge": "^2.0.1",
    "escape-string-regexp": "^1.0.0",
    "object-path": "^0.11.4",
    "require-all": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.51",
    "discord.js": "^11.2.0",
    "discord.js-docgen": "hydrabolt/discord.js-docgen",
    "eslint": "^4.0.0",
    "i18next": "^10.0.7",
    "js-yaml": "^3.10.0",
    "jsdoc-strip-async-await": "^0.1.0",
    "mongoose": "^4.13.2",
    "node-cache": "^4.1.1",
    "redis": "^2.8.0",
    "sqlite": "^2.0.0",
    "typescript": "^2.0.0"
  },
  "peerDependencies": {
    "discord.js": "^11.2.0",
    "i18next": "^10.0.7",
    "js-yaml": "^3.10.0",
    "mongoose": "^4.13.2",
    "node-cache": "^4.1.1",
    "redis": "^2.8.0",
    "sqlite": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
